# Discord OAuth2 (required)
DISCORD_CLIENT_ID=replace_with_discord_client_id
DISCORD_CLIENT_SECRET=replace_with_discord_client_secret
# Must match Discord Developer Portal redirect exactly.
# Example: https://projectmisfits.404development.dev/api/auth/discord/callback
DISCORD_REDIRECT_URI=https://projectmisfits.404development.dev/api/auth/discord/callback

# Shared PostgreSQL (same DB used by PM_API)
DATABASE_URL=postgresql://postgres:password@host:5432/railway
# Railway commonly requires SSL=true
DATABASE_SSL=true

# Optional bootstrap admins for /admin access (comma-separated Discord IDs)
ADMIN_DISCORD_IDS=
